{
  "framework": null,
  "buildCommand": "bash build.sh",
  "outputDirectory": "public",
  "installCommand": "echo 'Dependencies will be installed in build script'",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(wasm|dll|pdb|dat|blat)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    },
    {
      "source": "/(.*)\\.(br)",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "br"
        }
      ]
    }
  ]
}